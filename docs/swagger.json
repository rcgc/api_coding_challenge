{
    "swagger": "2.0",
    "info": {
        "description": "Simple swagger implementation in Go HTTP",
        "title": "Cars Restful API with Swagger",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "Roberto Guzm√°n",
            "email": "roberto140298@gmail.com"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "https://opensource.org/license/mit/"
        },
        "version": "0.1"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/cars": {
            "get": {
                "description": "Retrieves all the cars stored in the database",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "car"
                ],
                "summary": "List cars",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "array",
                            "items": {
                                "type": "array",
                                "items": {
                                    "$ref": "#/definitions/main.Car"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "main.Car": {
            "description": "car information",
            "type": "object",
            "properties": {
                "category": {
                    "type": "string"
                },
                "color": {
                    "type": "string"
                },
                "id": {
                    "type": "string"
                },
                "make": {
                    "type": "string"
                },
                "mileage": {
                    "type": "number"
                },
                "model": {
                    "type": "string"
                },
                "package": {
                    "type": "string"
                },
                "price": {
                    "type": "number"
                },
                "year": {
                    "type": "integer"
                }
            }
        }
    },
    "securityDefinitions": {
        "BasicAuth": {
            "type": "basic"
        }
    },
    "externalDocs": {
        "description": "OpenAPI",
        "url": "https://swagger.io/resources/open-api/"
    }
}